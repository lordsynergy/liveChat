<p id="notice"><%= notice %></p>
<p id="alert"><%= alert %></p>

<div class="mt-2 mb-3">
  <%= render 'rooms/form', room: @room %>
</div>

<h2>Rooms</h2>

<ul class="mt-4 list-unstyled">
  <% @rooms.each do |room| %>
    <%= turbo_stream_from "rooms" %>
    <div id="rooms">
      <%= render partial: 'room', locals: { room: room } %>
    </div>
  <% end %>
</ul>
